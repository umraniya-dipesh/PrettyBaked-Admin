        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminPrettyBacked">
        <activity android:name=".EditActivity.EditCategoryActivity"></activity>
        <activity android:name=".InsertCategoryActivity" />
        <activity android:name=".admin_ordersList_Activity" />
        <activity android:name=".InsertSubCategoryActivity" />
        <activity android:name=".EditActivity.EditSubCategoryActivity" />
        <activity android:name=".admin_subcategoryList_Activity" />
        <activity android:name=".adminCategoryListActivity" />
        <activity android:name=".EditActivity.EditUserActivity" />
        <activity android:name=".ViewUserListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>